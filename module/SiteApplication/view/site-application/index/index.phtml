<div ng id="nossoCorretores" class="reveal-modal" data-reveal ng-controller="CorretoresCtrl">

	<div class="row margem_bottom_10 padding_left_10" >
		<h1 class="titulo-modal titulo-parte-home-light-green ">Nossos Colaboradores {{ currentPage }}</h1>
		<p class="light_gray_f subtitulo">Entre em contato com um de nossos colaboradores:</p>
	</div>

	<div ng-show="loadingCorretores" class="carregando_informacao texto_centralizado">
		<img src="/img/uncompressed/carregando.gif" alt="">
	</div>

	<div class="ng-cloak" pagination-id="colabor" dir-paginate="corretor in corretores | filter:q | itemsPerPage: pageSize: 'colabor'" current-page="currentPage">

		<div class="row">
			<div class="large-3 small-12 columns">
				<span ng-show="corretor.foto.length">
					{{ corretor.foto }}
				</span>
				<span ng-hide="corretor.foto.length">
					<img src="/img/corretores/sem-foto.png" alt="Sem Foto Corretor">
				</span>
			</div>
			<div class="large-8 large-offset-1 small-12 columns">
				<table style="width: 100%">
					<tbody>
						<tr>
							<td><strong>Nome:</strong></td>
							<td><span>{{ corretor.nome }}</span><br></td>
						</tr>

						<span ng-show="corretor.area.length > 0">
							<tr>
								<td><strong>Area:</strong></td>
								<td><span>{{ corretor.area }}</span></td>
							</tr>
						</span>
						
						<span ng-show="corretor.cargo.length > 0">
							<tr>
								<td><strong>Cargo:</strong></td>
								<td><span>{{ corretor.cargo }}</span></td>
							</tr>
						</span>
						
						<span ng-show="corretor.filial.length > 0">
							<tr>
								<td><strong>Filial:</strong></td>
								<td><span>{{ corretor.filial }}</span></td>
							</tr>
						</span>

						<tr>
							<td><strong>E-mail:</strong></td>
							<td><span>{{ corretor.email }}</span></td>
						</tr>
						
						<tr>
							<td>
								<strong>
									<ng-pluralize count="corretor.telefones.length"
									                 when="{'0': 'Sem telefone!',
									                     'one': '1 Telefone:',
									                     'other': '{} Telefones:'}">
									</ng-pluralize>
								</strong>
							</td>
							<td>
								<span ng-repeat="telefone in corretor.telefones">
									
									<div ng-if="telefone.tipo=='celular'"><span class="icon-mobile">{{ telefone.num }}</span></div>
									<div ng-if="telefone.tipo=='telefone'"><span class="icon-phone">{{ telefone.num }}</span></div>
									<div ng-if="telefone.tipo=='fax'"><span class="icon-uniE603">{{ telefone.num }}</span></div>
									
								</span>
							</td>
						</tr>
					</tbody>
				</table>
			</div>
		</div>
	</div>

	<dir-pagination-controls pagination-id="colabor" boundary-links="true" on-page-change="pageChangeHandler(newPageNumber)" template-url="/templates/dirPagination.tpl.html" class="ng-cloak"></dir-pagination-controls>

	<a class="close-reveal-modal">&#215;</a>
</div> <!-- fim modal nossoCorretores -->

